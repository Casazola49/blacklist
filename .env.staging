# Firebase Configuration - Staging
VITE_FIREBASE_API_KEY=your_staging_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_staging_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_staging_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_staging_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_staging_sender_id
VITE_FIREBASE_APP_ID=your_staging_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_staging_measurement_id

# Application Configuration - Staging
VITE_APP_NAME="The Blacklist - Staging"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENVIRONMENT="staging"
VITE_APP_DEBUG="true"
VITE_APP_LOG_LEVEL="debug"

# API Configuration
VITE_API_BASE_URL="https://staging-api.theblacklist.app"
VITE_API_TIMEOUT="10000"

# Feature Flags
VITE_FEATURE_ANALYTICS="false"
VITE_FEATURE_ERROR_REPORTING="true"
VITE_FEATURE_PERFORMANCE_MONITORING="true"
VITE_FEATURE_MAINTENANCE_MODE="false"

# Security
VITE_ENABLE_DEVTOOLS="true"
VITE_STRICT_CSP="false"